{
  "name": "react-snuggle",
  "version": "1.0.6",
  "description": "Simple masonry-like css-based React Component",
  "keywords": [
    "grid",
    "masonry",
    "columns",
    "tiles",
    "pinterest",
    "css",
    "responsive",
    "react"
  ],
  "main": "dist/react-snuggle.cjs.js",
  "module": "dist/react-snuggle.es.js",
  "jsnext:main": "dist/react-snuggle.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "start-storybook -p 6006",
    "build": "rm -fr ./dist && rollup -c",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "test": "npm run lint && npm run tsc",
    "publish:storybook": "build-storybook && storybook-to-ghpages",
    "format": "prettier \"(src|stories)/**/*.+(ts|tsx|js|jsx|json|yml|yaml|md|mdx)\" --write",
    "tsc": "node_modules/.bin/tsc",
    "semantic-release": "semantic-release",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/significa/react-snuggle.git"
  },
  "author": "Significa Lda.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/significa/react-snuggle/issues"
  },
  "homepage": "https://github.com/significa/react-snuggle#readme",
  "devDependencies": {
    "@babel/runtime": "^7.9.6",
    "@significa/eslint-config": "0.0.17",
    "@significa/prettier-config": "0.0.17",
    "@significa/tsconfig-config": "0.0.17",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@types/react-lazy-load-image-component": "^1.5.1",
    "@types/scrollreveal": "0.0.6",
    "@types/styled-components": "^5.1.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "babel-runtime": "^6.26.0",
    "commitizen": "^4.1.5",
    "css-loader": "^3.5.3",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.5",
    "react": "^16.13.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.13.1",
    "react-lazy-load-image-component": "^1.5.0",
    "rollup": "2.7.6",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "rollup-plugin-uglify-es": "^0.0.1",
    "scrollreveal": "^4.0.6",
    "semantic-release": "^17.0.7",
    "styled-components": "^5.1.0",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "react": "^15.0.0-0 || ^16.0.0-0"
  },
  "dependencies": {},
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "resolutions": {
    "@types/react": "^16.7.22"
  }
}
